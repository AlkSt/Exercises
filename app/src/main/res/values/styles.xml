<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="RadButtonFill" parent="@style/Widget.AppCompat.Button.Colored">
        <item name="colorButtonNormal">@color/radButton</item>
    </style>

    <style name="BlueButtonFill" parent="@style/Widget.AppCompat.Button.Colored">
        <item name="colorButtonNormal">@color/blueButton</item>
    </style>

    <style name="GreenButtonFill" parent="@style/Widget.AppCompat.Button.Colored">
        <item name="colorButtonNormal">@color/greenButton</item>
    </style>
    <style name="YellowButtonFill" parent="@style/Widget.AppCompat.Button.Colored">
        <item name="colorButtonNormal">@color/yellowButton</item>
    </style>
    <style name="SeaButtonFill" parent="@style/Widget.AppCompat.Button.Colored">
        <item name="colorButtonNormal">@color/seaColor</item>
    </style>

</resources>
